{
  "name": "d20-dicebot",
  "version": "0.0.1",
  "description": "Dice rolling bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkSpyro003/discord-dice-roller"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "test": "npm-run-all -s lint test:mocha",
    "test:mocha": "nyc mocha --full-trace --recursive src/**/*.spec.ts",
    "lint": "tslint -c tslint.json src/**/*.ts --exclude src/**/*.spec.ts"
  },
  "nyc": {
    "lines": 97,
    "statements": 97,
    "functions": 97,
    "branches": 97,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true
  },
  "author": "Christina Lena Korosec <christina.korosec@ds003.info>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.5.1",
    "ini": "^1.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.6",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
